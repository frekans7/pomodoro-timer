{
  "manifest_version": 3,
  "name": "Pomodoro Timer",
  "author": "@frekans7",
  "description": "Pomodoro Timer App",
  "version": "1.2",
  "icons": {
    "16": "pomodoro.png",
    "48": "pomodoro.png",
    "128": "pomodoro.png"
  },
  "background": {
    "service_worker": "chrome/background.js"
  },
  "action": {
    "default_title": "Pomodoro",
    "default_icon": "pomodoro.png"
  },
  "permissions": ["notifications"]
}
